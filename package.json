{
  "name": "react-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@vercel/analytics": "^1.0.0",
    "formik": "^2.2.9",
    "i18next": "^22.5.0",
    "react": "^16.13.1",
    "react-blob": "^2.1.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^16.13.1",
    "react-ga": "^3.3.1",
    "react-i18next": "^12.3.1",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.7",
    "react-router": "^6.11.2",
    "react-router-dom": "^6.11.2",
    "react-scripts": "^3.4.3",
    "tailwindcss": "^1.4.6",
    "yup": "^1.1.1"
  },
  "scripts": {
    "purge:css": "cross-env NODE_ENV=production postcss src/tailwind.css -o src/index.css",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "watch:css": "postcss src/tailwind.css -o src/index.css -w",
    "start": "npm run build:css && react-scripts --openssl-legacy-provider start",
    "build": "npm run purge:css && react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "customize-cra": "^1.0.0",
    "postcss-cli": "^7.1.1",
    "react-app-rewired": "^2.2.1"
  }
}
